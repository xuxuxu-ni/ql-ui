{
  "name": "ql-ui-project",
  "version": "0.0.0",
  "description": "ql-ui组件库",
  "scripts": {
    "dev": "pnpm -C examples dev",
    "dev:docs": "pnpm -C docs run docs:dev",
    "build": "pnpm -C packages run build && pnpm -C packages/theme-chalk run build",
    "test": "echo \"Error: no test specified\" && exit 1",
